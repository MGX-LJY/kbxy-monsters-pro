        "有\\d+%?机会.*?{UP}.*?{SELF}.*?(法术|魔法)"
      ],
      "buf_spd_up": [
        "{SELF}.*?速度.*?{UP}|{SELF}.*?(加速|迅捷|敏捷提升|加快速度)",
        "{UP}.*?{SELF}.*?速度",
        "速度(?:{SEP}(?:攻击|防御|法术|魔法|抗性))*?各{UP}{LEVEL}"
      ],
      "buf_def_up": [
        "{SELF}.*?(防御|防御力).*?{UP}|{SELF}.*?(护甲|硬化|铁壁)",
        "{UP}.*?{SELF}.*?(防御|防御力)",
        "(防御|防御力)(?:{SEP}(?:攻击|法术|魔法|速度|抗性))*?各{UP}{LEVEL}"
      ],
      "buf_res_up": [
        "{SELF}.*?(抗性|抗性值).*?{UP}|{SELF}.*?抗性增强|{SELF}.*?减易伤",
        "{UP}.*?{SELF}.*?(抗性|抗性值)",
        "(抗性|抗性值)(?:{SEP}(?:攻击|防御|速度|法术|魔法))*?各{UP}{LEVEL}"
      ],
      "buf_acc_up": [
        "{SELF}.*?命中率.*?{UP}",
        "{UP}.*?{SELF}.*?命中率",
        "命中率(?:{SEP}(?:暴击率|攻击|防御|速度|抗性|法术|魔法))*?各{UP}{LEVEL}"
      ],
      "buf_crit_up": [
        "{SELF}.*?(暴击|暴击率|会心).*?{UP}",
        "(必定暴击|命中时必定暴击)",
        "{UP}.*?{SELF}.*?(暴击|暴击率|会心)",
        "暴击率(?:{SEP}(?:命中率|攻击|防御|速度|抗性|法术|魔法))*?各{UP}{LEVEL}"
      ],
      "buf_heal": [
        "(回复|治疗|恢复).*?({SELF}|自身体力|自身HP|自身生命|自身最大血量)",
        "给对手造成伤害的\\s*1/2\\s*回复",
        "(?:[一二三四五六七八九十]|\\d+)\\s*回合内.*?每回合.*?(回复|恢复)"
      ],
      "buf_shield": [
        "护盾|护体|结界",
        "(所受|受到).*(法术|物理)?伤害.*(减少|降低|减半|减免|降低\\d+%|减少\\d+%|减)(?!.*敌方)",
        "伤害(减少|降低|减半|减免|降低\\d+%|减少\\d+%)",
        "减伤(?!.*敌方)|庇护|保护"
      ],
      "buf_purify": [
