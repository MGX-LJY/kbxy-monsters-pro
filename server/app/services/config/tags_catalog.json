  "patterns": {
    "global_macros": {
      "CN_NUM": "[一二两三四五六七八九十百千]+",
      "SELF": "(?:自身|自我|自己|本方|我方)",
      "ENEMY": "(?:对方|对手|敌(?:人|方))",
      "UP": "(?:提升|提高|上升|增强|增加|加成|升高|强化)",
      "DOWN": "(?:下降|降低|减少|衰减|减弱)",
      "LEVEL": "(?:\\s*(?:{CN_NUM}|\\d+)\\s*级)?",
      "ONE_OR_TWO": "(?:一|1|两|2|一或两|1或2|1-2|1～2|1~2)",
      "SEP": "(?:\\s*[、，,/和与及]+\\s*)"
    },
    "by_code": {
      "buf_atk_up": [
        "{SELF}.*?攻击.*?{UP}",
        "{UP}.*?{SELF}.*?攻击",
        "攻击(?:{SEP}(?:法术|魔法|防御|速度|抗性|命中率|暴击率))*?各{UP}{LEVEL}",
        "有\\d+%?机会.*?{UP}.*?{SELF}.*?攻击"
      ],
      "buf_mag_up": [
        "{SELF}.*?(法术|魔法).*?{UP}",
        "{UP}.*?{SELF}.*?(法术|魔法)",
